{
  "name": "datathletics-site",
  "version": "0.0.1",
  "description": "Data Athletics Web Site",
  "main": "index.js",
  "keywords": [
    "ironman",
    "athletics",
    "triathlon"
  ],
  "scripts": {
    "up": "DEBUG_COLORS=true nodemon --harmony application.js",
    "start": "node --harmony application.js",
    "watch": "chmod +x scripts/watch.sh && scripts/./watch.sh",
    "build": "chmod +x scripts/build.sh && scripts/./build.sh",
    "clean": "rm -rf www/assets/bundle/*.js"
    "postinstall": "./node_modules/.bin/browserify www/app/app.js | uglifyjs -mc > www/assets/bundle/bundle.js"
  },
  "author": "Abhishek Kapatkar <cabhishek@gmail.com> (http://cabhishek.com/)",
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:cabhishek/datathletics-site.git"
  },
  "dependencies": {
    "koa": "~0.5.1",
    "underscore": "~1.6.0",
    "backbone": "~1.1.2",
    "swig": "~1.3.2",
    "browserify": "~3.32.1",
    "co-views": "~0.2.0",
    "koa-logger": "~1.2.0",
    "koa-static": "~1.4.3",
    "koa-router": "~3.1.1",
    "koa-route": "~1.1.0",
    "koa-mount": "~1.2.3",
    "uglify-js": "~2.4.13",
    "jquery": "~2.1.0",
    "bookshelf": "~0.6.8",
    "log": "~1.4.0",
    "mysql": "~2.1.1",
    "co-body": "0.0.1",
    "backbone-relational": "~0.8.6",
    "backbone-validation": "~0.9.1",
    "co": "~3.0.5",
    "thunkify": "0.0.1",
    "async": "~0.2.10"
  },
  "engines": {
    "node": "0.11.x"
  },
  "license": "BSD-2-Clause"
}
